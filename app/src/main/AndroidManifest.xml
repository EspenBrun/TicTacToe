<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="ebru.mainActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlayersActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="ebru.playersActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TicTacToeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="ebru.ticTacToeActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity><activity
        android:name=".SummaryActivity"
        android:label="@string/app_name">
        <intent-filter>
            <action android:name="ebru.summaryActivity" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>
    </application>

</manifest>